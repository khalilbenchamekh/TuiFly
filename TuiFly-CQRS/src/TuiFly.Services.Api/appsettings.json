{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost,1433;Database=TUIFLY;User Id=sa;Password=YourStrongPassword;MultipleActiveResultSets=true"
    //"DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=TUIFLY;;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "CorsPolicy": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:3001"
    ]
  },
  "Swagger": {
    "Version": "v1",
    "Title": "TuiFly Project",
    "Endpoint": "/swagger/v1/swagger.json",
    "Description": "TuiFly API Swagger surface",
    "Bearer": {
      "Description": "Input the JWT like: Bearer {your token}",
      "Name": "Authorization",
      "Scheme": "Bearer",
      "BearerFormat": "JWT",
      "In": "Header",
      "Type": "ApiKey"
    }
  },
  "Localization": {
    "DefaultCulture": "en-US",
    "SupportedCultures": [
      "en-US",
      "fr-FR"
    ]
  }
}
